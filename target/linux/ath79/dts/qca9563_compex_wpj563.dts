/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

#include "qca956x.dtsi"

/ {
	model = "Compex WPJ563";
	compatible = "compex,wpj563", "qca,qca9563";

	chosen {
		bootargs = "console=ttyS0,115200n8";
	};

	leds {
		compatible = "gpio-leds";

		sig1 {
			label = "wpj563:green:sig1";
			gpios = <&gpio 1 GPIO_ACTIVE_LOW>;
		};

		sig2 {
			label = "wpj563:green:sig2";
			gpios = <&gpio 5 GPIO_ACTIVE_LOW>;
		};

		sig3 {
			label = "wpj563:green:sig4";
			gpios = <&gpio 6 GPIO_ACTIVE_LOW>;
		};

		sig4 {
			label = "wpj563:green:sig4";
			gpios = <&gpio 7 GPIO_ACTIVE_LOW>;
		};
	};

	keys {
		compatible = "gpio-keys";

		reset {
			linux,code = <KEY_RESTART>;
			gpios = <&gpio 2 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};
	};
};

&pcie {
	status = "okay";
};

&spi {
	status = "okay";

	num-cs = <1>;

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <25000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			uboot: partition@0 {
				label = "u-boot";
				reg = <0x000000 0x030000>;
				read-only;
			};

			partition@30000 {
				compatible = "denx,uimage";
				label = "firmware";
				reg = <0x030000 0xfc0000>;
			};

			art: partition@ff0000 {
				label = "art";
				reg = <0xff0000 0x010000>;
				read-only;
			};
		};
	};
};

&mdio0 {
	status = "okay";

	phy-mask = <0>;
	phy0: ethernet-phy@0 {
		reg = <0>;
		phy-mode = "sgmii";
		qca,mib-poll-interval = <500>;

		qca,ar8327-initvals = <
			0x04 0x00080080 /* PORT0 PAD MODE CTRL */
			0x7c 0x0000007e /* PORT0_STATUS */
		>;
	};
};

&eth0 {
	status = "okay";
	// TODO mac wrong

	pll-data = <0x03000101 0x00000101 0x00001919>;

	mtd-mac-address = <&uboot 0x10>;
	phy-mode = "sgmii";
	phy-handle = <&phy0>;
};

&eth1 {
	// TODO mac wrong and doesn't work
	status = "okay";
};

&usb_phy0 {
	status = "okay";
};

&usb0 {
	status = "okay";

	// TODO test this
	#address-cells = <1>;
	#size-cells = <0>;

	port@1 {
		reg = <1>;
		#trigger-source-cells = <0>;
	};
};

&uart {
	status = "okay";
};

&gpio {
	status = "okay";
};

&wmac {
	status = "okay";
	mtd-cal-data = <&art 0x1000>;
	mtd-mac-address = <&uboot 0x18>;
};
